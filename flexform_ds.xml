<T3DataStructure>
    <meta type="array">
        <langDisable>1</langDisable>
    </meta>
    <sheets type="array">
        <sDEF type="array">
            <ROOT type="array">
                <TCEforms type="array">
                    <sheetTitle>LLL:EXT:searchbar/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el type="array">
                    <pluginName>
                        <TCEforms>
                            <config>
                                <type_t3dev>input</type_t3dev>
                                <size></size>
                                <max></max>
                                <type>input</type>
                            </config>
                            <label>LLL:EXT:searchbar/locallang_db.xml:tt_content.pi_flexform.field_PluginName</label>
                        </TCEforms>
                    </pluginName>
                    <pluginDescription>
                        <TCEforms>
                            <config>
                                <type_t3dev>input</type_t3dev>
                                <size></size>
                                <max></max>
                                <type>input</type>
                            </config>
                            <label>LLL:EXT:searchbar/locallang_db.xml:tt_content.pi_flexform.field_PluginDescription
                            </label>
                        </TCEforms>
                    </pluginDescription>
                    <pluginIcon>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:searchbar/locallang_db.xml:tt_content.pi_flexform.field_PluginIcon</label>
                            <config>
                                <type>group</type>
                                <internal_type>file</internal_type>
                                <allowed>png,jpg</allowed>
                                <max_size>500</max_size>
                                <uploadfolder>uploads/tx_searchbar</uploadfolder>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </pluginIcon>
                    <contentProviderText>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>Content:</label>
                            <config>
                                <type>text</type>
                                <default>Click at ###LINK###</default>
                            </config>
                        </TCEforms>
                    </contentProviderText>

                    <linkname>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>Linkname:</label>
                            <config>
                                <type>input</type>
                                <default>LINKNAME</default>
                            </config>
                        </TCEforms>
                    </linkname>

                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>